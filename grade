#include<stdio.h>
main()
{
    int a;
    printf("Enter the number = ");
    scanf("%d",&a);
    if(a>=33 && a<40)
    {
        printf("Grade D");
    }
    else if(a>=40 && a<50)
    {
        printf("Grade C");
    }
    else if(a>=50 && a<60)
    {
        printf("Grade B");
    }
    else if(a>=60 && a<70)
    {
        printf("Grade A-");
    }
    else if(a>=70 && a<80)
    {
        printf("Grade A");
    }
    else if (a>=80 && a<=100)
    {
        printf("Grade A+");
    }
    else if(a>100)
    {
        printf("Error");
    }
    else
    {
        printf("Failed");
    }
    return 0;
}
